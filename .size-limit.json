[
  {
    "name": "Main Bundle",
    "path": ".next/static/chunks/main-*.js",
    "limit": "90 KB",
    "gzip": true,
    "brotli": false
  },
  {
    "name": "Framework Bundle",
    "path": ".next/static/chunks/framework-*.js",
    "limit": "45 KB",
    "gzip": true,
    "brotli": false
  },
  {
    "name": "Pages Bundle",
    "path": ".next/static/chunks/pages/*.js",
    "limit": "30 KB",
    "gzip": true,
    "brotli": false
  },
  {
    "name": "Commons Bundle",
    "path": ".next/static/chunks/commons-*.js",
    "limit": "20 KB",
    "gzip": true,
    "brotli": false
  },
  {
    "name": "Total App Size",
    "path": [
      ".next/static/chunks/**/*.js",
      ".next/static/css/**/*.css"
    ],
    "limit": "250 KB",
    "gzip": true,
    "brotli": false
  },
  {
    "name": "CSS Bundle",
    "path": ".next/static/css/*.css",
    "limit": "20 KB",
    "gzip": true,
    "brotli": false
  },
  {
    "name": "First Load JS",
    "path": [
      ".next/static/chunks/main-*.js",
      ".next/static/chunks/framework-*.js",
      ".next/static/chunks/pages/index-*.js"
    ],
    "limit": "150 KB",
    "gzip": true,
    "brotli": false
  },
  {
    "name": "Service Worker",
    "path": "public/sw.js",
    "limit": "50 KB",
    "gzip": true,
    "brotli": false
  }
]